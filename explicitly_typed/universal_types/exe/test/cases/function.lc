id = \A . \x : A . x ;

app = \A . \B . \f : A -> B . \x : A . f x ;

rev_app = \A . \B . \x : A . \f : A -> B . f x ;

compose = \A . \B . \C . \f : A -> B . \g : C -> A . \x : C . f (g x) ;

arg_swap = \A . \B . \C . \f : A -> B -> C . \x : B . \y : A . f y x ;

rev_app' = \A . \B. arg_swap (A -> B) A B (app A B) ;

