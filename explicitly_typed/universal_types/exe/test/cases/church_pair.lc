pair = \A . \B . \f : A . \s : B . \C . \p : A -> B -> C . p f s ;

fst =
  \A . \B . \p : forall C . (A -> B -> C) -> C .
    p A (\f : A . \s : B . f) ;

snd =
  \A . \B . \p : forall C . (A -> B -> C) -> C .
    p B (\f : A . \s : B . s) ;

p = pair Bool Bool fls tru ;

fst Bool Bool p ;

snd Bool Bool p ;

p = pair Nat Nat zero one ;

fst Nat Nat p ;

snd Nat Nat p ;

p = pair Bool Nat fls zero ;

fst Bool Nat p ;

snd Bool Nat p ;
