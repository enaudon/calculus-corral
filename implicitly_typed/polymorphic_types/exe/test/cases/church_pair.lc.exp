pair
  : A -> B -> (A -> B -> C) -> C
  = \A . \B . \C . \x : A . \y : B . \p : A -> B -> C . p x y ;
fst
  : ((A -> B -> A) -> C) -> C
  = \A . \B . \C . \p : (A -> B -> A) -> C . p (\x : A . \y : B . x) ;
snd
  : ((A -> B -> B) -> C) -> C
  = \A . \B . \C . \p : (A -> B -> B) -> C . p (\x : A . \y : B . y) ;
