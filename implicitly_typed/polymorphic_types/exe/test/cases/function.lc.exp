id
  : A -> A
  = \A . \x : A . x ;
app
  : (A -> B) -> A -> B
  = \A . \B . \f : A -> B . \x : A . f x ;
rev_app
  : A -> (A -> B) -> B
  = \A . \B . \x : A . \f : A -> B . f x ;
compose
  : (A -> B) -> (C -> A) -> C -> B
  = \A . \B . \C . \f : A -> B . \g : C -> A . \x : C . f (g x) ;
arg_swap
  : (A -> B -> C) -> B -> A -> C
  = \A . \B . \C . \f : A -> B -> C . \x : B . \y : A . f y x ;
